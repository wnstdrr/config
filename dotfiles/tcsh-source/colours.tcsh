# special colors
set BOLD = "%{\033[1m%}"
set DIM = "%{\033[2m%}"
set BLINK = "%{\033[5m%}"
set INVERT = "%{\033[7m%}"
set HIDDEN = "%{\033[8m%}"
set DEFAULT = "%{\033[39m%}"
set RESET = "%{\033[0m%}"

# custom term colors
set RED = "%{\033[31m%}"
set ORANGE = "{%\033[38;5;208m%}"
set YELLOW = "%{\033[93m%}"
set GREEN = "%{\033[92m%}"
set BLUE = "%{\033[34m%}"
set MAGENTA = "%{\033[35m%}"
